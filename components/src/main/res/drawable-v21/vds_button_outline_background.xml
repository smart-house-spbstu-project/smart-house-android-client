<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
        android:color="@color/vds_gray_highlight" android:alpha="@dimen/vds_cloudy_opacity">
    <item android:id="@android:id/mask">
        <shape android:shape="rectangle">
            <solid android:color="@drawable/vds_button_outline_background_color" />
            <corners android:radius="@dimen/vds_padding_4" />
        </shape>
    </item>
    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <stroke android:color="@drawable/vds_button_outline_color" android:width="@dimen/vds_viewer_divider_height"/>
            <corners android:radius="@dimen/vds_padding_4"/>
            <solid android:color="@drawable/vds_button_outline_background_color"/>
        </shape>
    </item>
</ripple>